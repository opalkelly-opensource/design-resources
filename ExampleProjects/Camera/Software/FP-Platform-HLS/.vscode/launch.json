    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "name": "Attach FP-App",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "sourceMapPathOverrides":
            { 
                "webpack://frontpanel-camera-reactapp/./*": "${webRoot}/*", 
                "webpack:///*": "*"
            },
            "trace": true
        }
    ]
}
